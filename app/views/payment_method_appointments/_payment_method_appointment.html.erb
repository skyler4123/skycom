<div id="<%= dom_id payment_method_appointment %>" class="w-full sm:w-auto my-5 space-y-5">
  <div>
    <strong class="block font-medium mb-1">Payment method:</strong>
    <%= payment_method_appointment.payment_method_id %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Company:</strong>
    <%= payment_method_appointment.company_id %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Name:</strong>
    <%= payment_method_appointment.name %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Description:</strong>
    <%= payment_method_appointment.description %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Code:</strong>
    <%= payment_method_appointment.code %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Status:</strong>
    <%= payment_method_appointment.status %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Business type:</strong>
    <%= payment_method_appointment.business_type %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Discarded at:</strong>
    <%= payment_method_appointment.discarded_at %>
  </div>
</div>
