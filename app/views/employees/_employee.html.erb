<div id="<%= dom_id employee %>" class="w-full sm:w-auto my-5 space-y-5">
  <div>
    <strong class="block font-medium mb-1">User:</strong>
    <%= employee.user_id %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Company:</strong>
    <%= employee.company_id %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Name:</strong>
    <%= employee.name %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Description:</strong>
    <%= employee.description %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Code:</strong>
    <%= employee.code %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Status:</strong>
    <%= employee.status %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Business type:</strong>
    <%= employee.business_type %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Discarded at:</strong>
    <%= employee.discarded_at %>
  </div>
</div>
