<div id="<%= dom_id period %>" class="w-full sm:w-auto my-5 space-y-5">
  <div>
    <strong class="block font-medium mb-1">Company:</strong>
    <%= period.company_id %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Name:</strong>
    <%= period.name %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Description:</strong>
    <%= period.description %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Code:</strong>
    <%= period.code %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Duration:</strong>
    <%= period.duration %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Start at:</strong>
    <%= period.start_at %>
  </div>
  <div>
    <strong class="block font-medium mb-1">End at:</strong>
    <%= period.end_at %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Expire at:</strong>
    <%= period.expire_at %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Discarded at:</strong>
    <%= period.discarded_at %>
  </div>
</div>
