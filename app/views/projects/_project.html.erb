<div id="<%= dom_id project %>" class="w-full sm:w-auto my-5 space-y-5">
  <div>
    <strong class="block font-medium mb-1">Company:</strong>
    <%= project.company_id %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Project group:</strong>
    <%= project.project_group_id %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Name:</strong>
    <%= project.name %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Description:</strong>
    <%= project.description %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Code:</strong>
    <%= project.code %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Status:</strong>
    <%= project.status %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Business type:</strong>
    <%= project.business_type %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Discarded at:</strong>
    <%= project.discarded_at %>
  </div>
</div>
