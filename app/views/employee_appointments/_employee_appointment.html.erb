<div id="<%= dom_id employee_appointment %>" class="w-full sm:w-auto my-5 space-y-5">
  <div>
    <strong class="block font-medium mb-1">Employee:</strong>
    <%= employee_appointment.employee_id %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Appoint from:</strong>
    <%= employee_appointment.appoint_from_id %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Appoint to:</strong>
    <%= employee_appointment.appoint_to_id %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Appoint for:</strong>
    <%= employee_appointment.appoint_for_id %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Appoint by:</strong>
    <%= employee_appointment.appoint_by_id %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Name:</strong>
    <%= employee_appointment.name %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Description:</strong>
    <%= employee_appointment.description %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Code:</strong>
    <%= employee_appointment.code %>
  </div>
</div>
