<div id="<%= dom_id company %>" class="w-full sm:w-auto my-5 space-y-5">
  <div>
    <strong class="block font-medium mb-1">User:</strong>
    <%= company.user_id %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Parent company:</strong>
    <%= company.parent_company_id %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Name:</strong>
    <%= company.name %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Description:</strong>
    <%= company.description %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Status:</strong>
    <%= company.status %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Kind:</strong>
    <%= company.kind %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Business type:</strong>
    <%= company.business_type %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Discarded at:</strong>
    <%= company.discarded_at %>
  </div>
</div>
