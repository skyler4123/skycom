<div id="<%= dom_id product %>" class="w-full sm:w-auto my-5 space-y-5">
  <div>
    <strong class="block font-medium mb-1">Company:</strong>
    <%= product.company_id %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Brand:</strong>
    <%= product.brand_id %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Name:</strong>
    <%= product.name %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Description:</strong>
    <%= product.description %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Code:</strong>
    <%= product.code %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Sku:</strong>
    <%= product.sku %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Barcode:</strong>
    <%= product.barcode %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Upc:</strong>
    <%= product.upc %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Ean:</strong>
    <%= product.ean %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Manufacturer code:</strong>
    <%= product.manufacturer_code %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Serial number:</strong>
    <%= product.serial_number %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Batch number:</strong>
    <%= product.batch_number %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Expiration date:</strong>
    <%= product.expiration_date %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Status:</strong>
    <%= product.status %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Business type:</strong>
    <%= product.business_type %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Discarded at:</strong>
    <%= product.discarded_at %>
  </div>
</div>
