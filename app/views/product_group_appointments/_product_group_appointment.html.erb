<div id="<%= dom_id product_group_appointment %>" class="w-full sm:w-auto my-5 space-y-5">
  <div>
    <strong class="block font-medium mb-1">Product group:</strong>
    <%= product_group_appointment.product_group_id %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Appoint to:</strong>
    <%= product_group_appointment.appoint_to_id %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Name:</strong>
    <%= product_group_appointment.name %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Description:</strong>
    <%= product_group_appointment.description %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Code:</strong>
    <%= product_group_appointment.code %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Status:</strong>
    <%= product_group_appointment.status %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Business type:</strong>
    <%= product_group_appointment.business_type %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Discarded at:</strong>
    <%= product_group_appointment.discarded_at %>
  </div>
</div>
