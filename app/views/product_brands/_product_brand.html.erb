<div id="<%= dom_id product_brand %>" class="w-full sm:w-auto my-5 space-y-5">
  <div>
    <strong class="block font-medium mb-1">Name:</strong>
    <%= product_brand.name %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Description:</strong>
    <%= product_brand.description %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Status:</strong>
    <%= product_brand.status %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Business type:</strong>
    <%= product_brand.business_type %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Discarded at:</strong>
    <%= product_brand.discarded_at %>
  </div>
</div>
