<div id="<%= dom_id order_item_appointment %>" class="w-full sm:w-auto my-5 space-y-5">
  <div>
    <strong class="block font-medium mb-1">Order:</strong>
    <%= order_item_appointment.order_id %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Appoint to:</strong>
    <%= order_item_appointment.appoint_to_id %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Unit price:</strong>
    <%= order_item_appointment.unit_price %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Quantity:</strong>
    <%= order_item_appointment.quantity %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Total price:</strong>
    <%= order_item_appointment.total_price %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Name:</strong>
    <%= order_item_appointment.name %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Description:</strong>
    <%= order_item_appointment.description %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Status:</strong>
    <%= order_item_appointment.status %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Business type:</strong>
    <%= order_item_appointment.business_type %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Discarded at:</strong>
    <%= order_item_appointment.discarded_at %>
  </div>
</div>
