<div id="<%= dom_id subscription %>" class="w-full sm:w-auto my-5 space-y-5">
  <div>
    <strong class="block font-medium mb-1">Subscription group:</strong>
    <%= subscription.subscription_group_id %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Company group:</strong>
    <%= subscription.company_group_id %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Company:</strong>
    <%= subscription.company_id %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Name:</strong>
    <%= subscription.name %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Description:</strong>
    <%= subscription.description %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Code:</strong>
    <%= subscription.code %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Status:</strong>
    <%= subscription.status %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Business type:</strong>
    <%= subscription.business_type %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Discarded at:</strong>
    <%= subscription.discarded_at %>
  </div>
</div>
