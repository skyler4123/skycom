<div id="<%= dom_id employee_group_appointment %>" class="w-full sm:w-auto my-5 space-y-5">
  <div>
    <strong class="block font-medium mb-1">Employee group:</strong>
    <%= employee_group_appointment.employee_group_id %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Appoint to:</strong>
    <%= employee_group_appointment.appoint_to_id %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Name:</strong>
    <%= employee_group_appointment.name %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Description:</strong>
    <%= employee_group_appointment.description %>
  </div>
</div>
